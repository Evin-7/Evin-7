{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      dialog: false,\n      newText: \"\",\n      texts: [] // Array to store the texts\n    };\n  },\n\n  mounted() {\n    this.SliderContent();\n  },\n  methods: {\n    openDialog() {\n      this.dialog = true;\n    },\n    closeDialog() {\n      this.dialog = false;\n      this.newText = \"\"; // Clear the text field when the dialog is closed\n    },\n\n    addText() {\n      // Call the API to add the text\n      axios.post(\"http://127.0.0.1:5000/api/add-text\", {\n        new_text: this.newText\n      }).then(response => {\n        console.log(response.data.message);\n        // Update the array of texts with the new text\n        this.SliderContent();\n        this.closeDialog(); // Close the dialog after adding the text\n      }).catch(err => {\n        console.error(err);\n      });\n    },\n    SliderContent() {\n      // Call the API to get the updated list of texts\n      axios.get(\"http://127.0.0.1:5000/api/get-updated-texts\").then(response => {\n        this.texts = response.data;\n      }).catch(err => {\n        console.error(err);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAiCA;AAEA;EACAA;IACA;MACAC;MACAC;MACAC;IACA;EACA;;EAEAC;IACA;EACA;EAEAC;IACAC;MACA;IACA;IAEAC;MACA;MACA;IACA;;IAEAC;MACA;MACAC,MACAC;QAAAC;MAAA,GACAC;QACAC;QACA;QACA;QACA;MACA,GACAC;QACAD;MACA;IACA;IAEAE;MACA;MACAN,MACAO,mDACAJ;QACA;MACA,GACAE;QACAD;MACA;IACA;EACA;AACA","names":["data","dialog","newText","texts","mounted","methods","openDialog","closeDialog","addText","axios","post","new_text","then","console","catch","SliderContent","get"],"sourceRoot":"src/components","sources":["HomePage.vue"],"sourcesContent":["<template>\n  <div>\n    <v-layout wrap>\n      <v-flex xs2> jkjk </v-flex>\n      <v-flex xs3>\n        <v-btn @click=\"openDialog\">Add Texts</v-btn>\n      </v-flex>\n    </v-layout>\n\n    <v-dialog v-model=\"dialog\" max-width=\"600px\">\n      <v-card>\n        <v-card-title>Add Text</v-card-title>\n        <v-card-text>\n          <v-text-field v-model=\"newText\" label=\"Enter Text\"></v-text-field>\n        </v-card-text>\n        <v-card-actions>\n          <v-btn @click=\"closeDialog\">Cancel</v-btn>\n          <v-btn @click=\"addText\" color=\"primary\">Add</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n\n    <v-layout wrap>\n      <v-flex xs12>\n        <ul>\n          <li v-for=\"text in texts\" :key=\"text\">{{ text }}</li>\n        </ul>\n      </v-flex>\n    </v-layout>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      dialog: false,\n      newText: \"\",\n      texts: [], // Array to store the texts\n    };\n  },\n\n  mounted() {\n    this.SliderContent();\n  },\n\n  methods: {\n    openDialog() {\n      this.dialog = true;\n    },\n\n    closeDialog() {\n      this.dialog = false;\n      this.newText = \"\"; // Clear the text field when the dialog is closed\n    },\n\n    addText() {\n      // Call the API to add the text\n      axios\n        .post(\"http://127.0.0.1:5000/api/add-text\", { new_text: this.newText })\n        .then((response) => {\n          console.log(response.data.message);\n          // Update the array of texts with the new text\n          this.SliderContent();\n          this.closeDialog(); // Close the dialog after adding the text\n        })\n        .catch((err) => {\n          console.error(err);\n        });\n    },\n\n    SliderContent() {\n      // Call the API to get the updated list of texts\n      axios\n        .get(\"http://127.0.0.1:5000/api/get-updated-texts\")\n        .then((response) => {\n          this.texts = response.data;\n        })\n        .catch((err) => {\n          console.error(err);\n        });\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}