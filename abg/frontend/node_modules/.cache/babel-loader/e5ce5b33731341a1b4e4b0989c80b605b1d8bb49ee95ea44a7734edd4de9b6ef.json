os({\n        method: \"GET\",\n        headers: {\n          siteId: \"6436362a65dc48eef99acd34\",\n        },\n        url: \"/user/services/list\",\n      })\n        .then((response) => {\n          //  this.$router.go();\n          this.services = response.data.data;\n          this.varx = response.data.data[0].link;\n          this.appLoading = false;\n        })\n        .catch((err) => {\n          this.ServerError = true;\n          console.log(err);\n        });\n    },\n\n    SliderContent() {\n    this.appLoading = true;\n    axios({\n      method: \"GET\",\n      headers: {\n        siteId: \"6436362a65dc48eef99acd34\",\n      },\n      url: \"http://127.0.0.1:5000/user/slider/details\", // Update the URL based on your Flask server\n    })\n      .then((response) => {\n        this.slider = response.data.data;\n        this.appLoading = false;\n      })\n      .catch((err) => {\n        this.ServerError = true;\n        console.error(err);\n      });\n  },\n    hompageContent() {\n      this.appLoading = true;\n      axios({\n        method: \"GET\",\n        headers: {\n          siteId: \"6436362a65dc48eef99acd34\",\n        },\n        url: \"/user/home/contents\",\n      })\n        .then((response) => {\n          //  this.$router.go();\n          this.list = response.data.data;\n          this.appLoading = false;\n        })\n        .catch((err) => {\n          this.ServerError = true;\n          console.log(err);\n        });\n    },\n  },\n};\n</script>\n\n\n<style>\n.loader {\n  z-index: 1000;\n  width: 100%;\n  height: 100%;\n  position: absolute;\n}\n.textalign {\n  text-align: center;\n}\n.index {\n  z-index: 1 !important;\n}\n.clr {\n  background-color: #37aee5;\n  color: #008bed;\n}\n.clr:hover {\n  background-color: white;\n  color: white;\n}\n.grad {\n  background-image: linear-gradient(to right, #fbfbfb, #fbfbfb);\n}\n.grad3 {\n  background-image: linear-gradient(to right, white, white);\n}\n.my-btn {\n  padding: 10px 20px; /* add 10px