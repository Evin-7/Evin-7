uppercase btn\"\n                    :style=\"{\n                      'background-color': hover ? '#0096FF' : 'white',\n                    }\"\n                  >\n                    <span class=\"buttonfont2\"> Go To Programs</span>\n                  </v-btn>\n                </v-hover>\n              </v-flex>\n            </v-layout>\n          </v-flex>\n          <v-flex lg4 sm5 xl3 xs10 pl-xs-0 pl-lg-12 align-self-center>\n            <v-card\n              height=\"400px\"\n              elevation=\"5\"\n              style=\"drop-shadow(0px 0px 100px rgba(0, 0, 0, 0.07));\"\n              class=\"rounded-xl grad2 pa-6 test2 text-center\"\n            >\n              <v-layout wrap justify-center>\n                <v-flex\n                  style=\"\n                    color: black;\n                    text-align: justify;\n                    font-size: 18px;\n                    font-weight: bolder;\n                  \"\n                  class=\"homefontsub2\"\n                  xs12\n                  text-left\n                >\n                  <span>{{ courses.tittle }}</span>\n                </v-flex>\n                <v-flex xs12 pt-2 pr-2>\n                  <v-img\n                    class=\"rounded-xl\"\n                    :height=\"\n                      $vuetify.breakpoint.name == 'xs'\n                        ? '200px'\n                        : $vuetify.breakpoint.name == 'sm'\n                        ? '150px'\n                        : $vuetify.breakpoint.name == 'md'\n                        ? '150px'\n                        : $vuetify.breakpoint.name == 'lg'\n                        ? '150px'\n                        : '150px'\n                    \"\n                    :width=\"\n                      $vuetify.breakpoint.name == 'xs'\n                        ? '370px'\n                        : $vuetify.breakpoint.name == 'sm'\n                        ? '1000px'\n                        : $vuetify.breakpoint.name == 'md'\n                        ? '1000px'\n                        : $vuetify.breakpoint.name == 'lg'\n                        ? '470px'\n                        : '470px'\n                    \"\n                    :src=\"mediaURLnew + courses.image\"\n                  ></v-img>\n                </v-flex>\n\n                <v-flex xs4 pt-2 text-left>\n                  <span>\n                    <v-rating\n                      size=\"80%\"\n                      color=\"yellow\"\n                      v-model=\"rating\"\n                      clearable\n                    ></v-rating>\n                  </span>\n                </v-flex>\n\n                <v-flex xs8 pt-2 pl-8>\n                  <strong><span>4.0</span></strong> &nbsp;\n                  <span>(65,354)</span>\n                </v-flex>\n\n                <v-flex\n                  xs12\n                  text-left\n                  class=\"mainfont\"\n                  style=\"color: black; text-align: justify; font-size: 15px\"\n                >\n                  <span>{{ courses.description }}</span>\n                </v-flex>\n              </v-layout>\n            </v-card>\n          </v-flex>\n        </v-layout>\n      </v-img>\n    </v-layout>\n    <v-layout wrap justify-start>\n      <v-flex xs1 text-left class=\"hidden-md-and-down\">\n        <center>\n          <v-img contain src=\"../assets/Images/auiconx2.png\"></v-img>\n        </center>\n      </v-flex>\n      <v-flex xs12 lg10 pa-14 text-center>\n        <span style=\"font-family: homefontsub2; color: #37aee5; font-size: 40px\"\n          >Our Pride\n        </span>\n        <v-layout pb-3 wrap justify-center>\n          <v-flex\n            md4\n            xl3\n            data-aos=\"zoom-in-up\"\n            data-aos-ease=\"ease\"\n            data-aos-duration=\"1500\"\n            data-aos-delay=\"500\"\n            xs12\n            pa-5\n            align-self-center\n            v-for=\"(item, i) in services\"\n            :key=\"i\"\n          >\n            <v-card\n              height=\"360px\"\n              style=\"box-shadow: 0px 4px 28px rgba(0, 0, 0, 0.25)\"\n              class=\"rounded-xl\"\n              color=\"white\"\n            >\n              <v-layout wrap justify-center fill-height pa-5>\n                <v-flex xs6>\n                  <center>\n                    <v-img\n                      width=\"100px\"\n                      contain\n                      height=\"100px\"\n                      :src=\"mediaURLnew + item.photo\"\n                    ></v-img>\n                  </center>\n                </v-flex>\n\n                <v-flex\n                  xs12\n                  style=\"\n                    font-family: subtitlefont;\n                    color: black;\n                    font-size: 12px;\n                    text-align: justify;\n                  \"\n                >\n                  <v-layout wrap justify-center>\n                    <v-flex\n                      pa-2\n                      text-center\n                      xs12\n                      class=\"homefontsub2\"\n                      style=\"color: black; font-size: 20px\"\n                      ><span>{{ item.tittle }}</span></v-flex\n                    >\n                    <v-flex\n                      pa-2\n                      xs12\n                      text-center\n                      class=\"mainfont\"\n                      style=\"color: black; font-size: 15px; text-align: justify\"\n                      ><span>{{ item.description }}</span></v-flex\n                    >\n                  </v-layout>\n                  <v-flex\n                    text-center\n                    pa-4\n                    style=\"color: white; font-size: 15px; text-decoration: none\"\n                  >\n                    <a\n                      target=\"_blank\"\n                      style=\"text-decoration: none\"\n                      :href=\"item.link\"\n                    >\n                      <v-btn\n                        width=\"50%\"\n                        class=\"no-uppercase\"\n                        color=\"rgb(0, 150, 255)\"\n                      >\n                        <span\n                          class=\"buttonfont\"\n                          style=\"\n                            color: white;\n                            font-size: 15px;\n                            text-decoration: none;\n                          \"\n                          >Visit</span\n                        >\n                      </v-btn>\n                    </a>\n                  </v-flex>\n                </v-flex>\n              </v-layout>\n            </v-card>\n          </v-flex>\n        </v-layout>\n      </v-flex>\n    </v-layout>\n\n    <v-layout wrap fill-height class=\"mainbg\">\n      <v-flex\n        data-aos=\"flip-right\"\n        data-aos-ease=\"ease\"\n        data-aos-duration=\"1500\"\n        data-aos-delay=\"200\"\n        pa-6\n        md12\n        lg12\n        text-center\n        style=\"\n          font-family: homefontsub2;\n          color: white;\n          text-align: justify;\n          line-height: 1.5;\n          font-size: 25px;\n          font-weight: bolder;\n        \"\n      >\n        <span\n          :style=\"\n            $vuetify.breakpoint.name == 'xs'\n              ? {\n                  'font-size': '20px',\n                }\n              : $vuetify.breakpoint.name == 'sm'\n              ? {\n                  'font-size': '17px',\n                }\n              : $vuetify.breakpoint.name == 'md'\n              ? {\n                  'font-size': '18px',\n                }\n              : {\n                  'font-size': '25px',\n                }\n          \"\n          >For any further queries please contact </span\n        >&nbsp; &nbsp;\n        <v-btn color=\"white\" rounded @click=\"$router.push('/ContactusPage')\">\n          <span class=\"