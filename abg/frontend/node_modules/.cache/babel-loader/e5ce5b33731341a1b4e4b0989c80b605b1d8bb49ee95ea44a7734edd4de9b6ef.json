-flex xs6 pr-10 pt-4 text-right>\n                      <v-hover v-slot=\"{ hover }\">\n                        <v-btn\n                          elevation=\"2\"\n                          @click=\"$router.push('/CoursesPage')\"\n                          width=\"180px\"\n                          class=\"no-uppercase zoom-card\"\n                          light\n                          :style=\"{\n                            'background-color': hover ? 'white' : '#0096FF',\n                          }\"\n                        >\n                          <span class=\"buttonfont\">Explore </span>\n                        </v-btn>\n                      </v-hover>\n                    </v-flex>\n                    <v-flex xs6 pr-10 text-left pt-4>\n                      <v-hover v-slot=\"{ hover }\">\n                        <v-btn\n                          @click=\"$router.push('/CoursesPage')\"\n                          class=\"zoom-card no-uppercase btn\"\n                          light\n                          :style=\"{\n                            'background-color': hover ? '#0096FF' : 'white',\n                          }\"\n                          outlined\n                        >\n                          <span class=\"buttonfont2\"> Our Programs </span>\n                        </v-btn>\n                      </v-hover>\n                    </v-flex>\n                  </v-layout>\n                </v-flex>\n              </v-layout>\n            </div>\n          </div>\n        </div>\n      </v-flex>\n    </v-layout>\n\n    <v-layout wrap style=\"height: 100vh\" class=\"hidden-md-and-up\">\n      <v-flex xs12>\n        <div class=\"hero-video\">\n          <video autoplay muted loop>\n            <source\n              contain\n              src=\"../assets/Images/video2.mp4\"\n              type=\"video/mp4\"\n            />\n          </video>\n          <div class=\"hero-text my-div\">\n            <v-layout wrap fill-height>\n       